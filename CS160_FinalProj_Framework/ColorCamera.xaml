<Window x:Class="CS160_FinalProj_Framework.ColorCamera"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ColorCamera" Height="300" Width="300" xmlns:my="clr-namespace:Microsoft.Samples.Kinect.WpfViewers;assembly=Microsoft.Samples.Kinect.WpfViewers">
    <Canvas>
        <my:KinectColorViewer Canvas.Left="0" Canvas.Top="0" Name="kinectColorViewer1" Height="261" Width="278" Kinect="{Binding Path=Kinect, ElementName=kinectSensorChooser1}" />
        <my:KinectSensorChooser Canvas.Left="0" Canvas.Top="24" Name="kinectSensorChooser1" Width="278" Height="208" />
    </Canvas>
</Window>
